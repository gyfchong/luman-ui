{
  "name": "@repo/cli",
  "version": "0.1.0",
  "description": "AI-native design system CLI",
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./api": "./src/api.ts"
  },
  "bin": {
    "luman": "./dist/index.js"
  },
  "scripts": {
    "build": "tsup src/index.ts src/api.ts --format esm --dts",
    "dev": "tsup src/index.ts src/api.ts --format esm --watch",
    "lint": "eslint . --max-warnings 0",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "citty": "^0.1.6",
    "@clack/prompts": "^0.7.0",
    "picocolors": "^1.1.1",
    "nypm": "^0.3.12",
    "pathe": "^1.1.2",
    "consola": "^3.2.3",
    "defu": "^6.1.4",
    "jiti": "^2.4.2",
    "pkg-types": "^1.2.1",
    "mlly": "^1.7.3",
    "@antfu/ni": "^0.23.0",
    "execa": "^9.5.2"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^22.15.3",
    "eslint": "^9.39.1",
    "tsup": "^8.3.5",
    "typescript": "5.9.2"
  }
}
